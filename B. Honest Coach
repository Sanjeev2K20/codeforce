Question: https://codeforces.com/problemset/problem/1360/B
code: https://onlinegdb.com/d6Kmy7udo

#include<iostream>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int a,b;
        cin>>a>>b;
        int A = min(a,b);
        int B = max(a,b);
        A*=2;
        int c = max(B,A);
        c*=c;
        cout<<c<<endl;
    }
}
