Question: https://codeforces.com/contest/1772/problem/A
code: https://onlinegdb.com/P6F4XGTwx

#include<iostream>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        string s;
        cin>>s;
        int a = (s[0]-'0');
        int b = (s[2]-'0');
        cout<<a+b<<endl;
    }
}

